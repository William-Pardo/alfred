{
    "name":  "alfred-agent",
    "version":  "0.1.0",
    "private":  true,
    "type":  "module",
    "main":  "alfred/index.js",
    "scripts":  {
                    "alfred":  "node -r dotenv/config tools/alfred/alfred/index.js",
                    "alfred:plan":  "node -r dotenv/config tools/alfred/alfred/index.js --plan-only",
                    "dev":  "vite",
                    "build":  "vite build",
                    "preview":  "vite preview --port 5173",
                    "lint":  "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
                    "test":  "node tests/smoke.js"
                },
    "dependencies":  {
                         "dotenv":  "^16.6.1",
                         "execa":  "^9.6.0",
                         "fast-glob":  "^3.3.3",
                         "node-fetch":  "^3.3.2",
                         "simple-git":  "^3.28.0"
                     },
    "devDependencies":  {
                            "@eslint/js":  "^9.9.0",
                            "eslint":  "^9.9.0",
                            "vite":  "^5.4.0"
                        }
}
